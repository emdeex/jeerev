</html>}

set html3 {
  !html
    head
      meta/charset=utf-8
      title: KAKU send
      [JScript includes ui]
      [JScript wrap $js]
    body
      % foreach x {1 2 3 4}
        p.toggles
          button#on$x: On $x
          button#off$x: Off $x
      p#group
        label: Group:
        % foreach x {I II III IV}
          input#g$x/type=radio/name=g/value=$x
          label/for=g$x: $x
      p
        label: House Code:
        select
        % foreach x {A B C D E F G H I J K L M N O P}
          option/value=$x: $x
}

